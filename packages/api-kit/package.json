{
	"name": "@monorepo-starter/api-kit",
	"private": true,
	"version": "1.0.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./errors": {
			"import": {
				"types": "./dist/errors.d.mts",
				"default": "./dist/errors.mjs"
			},
			"require": {
				"types": "./dist/errors.d.ts",
				"default": "./dist/errors.js"
			}
		},
		"./kv": {
			"import": {
				"types": "./dist/kv.d.mts",
				"default": "./dist/kv.mjs"
			},
			"require": {
				"types": "./dist/kv.d.ts",
				"default": "./dist/kv.js"
			}
		},
		"./auth": {
			"import": {
				"types": "./dist/auth.d.mts",
				"default": "./dist/auth.mjs"
			},
			"require": {
				"types": "./dist/auth.d.ts",
				"default": "./dist/auth.js"
			}
		},
		"./middlewares": {
			"import": {
				"types": "./dist/middlewares.d.mts",
				"default": "./dist/middlewares.mjs"
			},
			"require": {
				"types": "./dist/middlewares.d.ts",
				"default": "./dist/middlewares.js"
			}
		}
	},
	"scripts": {
		"build": "tsup src/index.ts src/errors.ts src/kv.ts src/auth.ts src/middlewares.ts --format esm,cjs --dts"
	},
	"devDependencies": {
		"@monorepo-starter/config-ts": "workspace:*",
		"@types/node": "^22.14.0",
		"@types/pg": "^8.11.11",
		"dotenv": "^16.4.7",
		"tsup": "^8.5.0",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@monorepo-starter/db": "workspace:*",
		"ioredis": "^5.7.0",
		"hono": "^4.7.6"
	}
}
